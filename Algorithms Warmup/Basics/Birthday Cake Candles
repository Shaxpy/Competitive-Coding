#include <bits/stdc++.h>
#include <vector>
#include <iostream>
#include <array>

using namespace std;
 int ar[100];
int Candles(int ar[],int n) {
    int r=0;
    for (int i=0;i<n;i++)
    {
        if (ar[i]==n)
        {
            r+=1;
        }

    }
      
return r;
}


int main()
{

    int ar_count;
    cin >> ar_count;
    
    for (int i=0;i<ar_count;i++)
    {
        
        cin>>ar[i];

    }
    
    int a=0;
    a=Candles(ar,ar_count);
    cout<<a;
    return 0;
}
