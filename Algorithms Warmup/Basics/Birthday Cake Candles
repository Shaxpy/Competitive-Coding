#include <bits/stdc++.h>
#include <vector>
#include <iostream>

using namespace std;
int ar[100];

int Candles(int ar) {
    int r=0;
    for (int i=0;i<ar.size();i++)
    {
        if (ar.size()==ar[i])
        {
            r=r+1;
        }

    }
      
return r;
}

int main()
{

    int ar_count;
    cin >> ar_count;
    
    for (int i=0;i<ar_count;i++)
    {
        
        cin>>ar[i];

    }
    
    int a=Candles(ar);
    cout<<a;
    return 0;
}
